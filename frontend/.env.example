# API Configuration
VITE_API_BASE_URL=https://your-api-gateway.amazonaws.com/prod
VITE_API_KEY=your-api-key-here
VITE_API_TIMEOUT=30000

# AWS Configuration (Optional)
VITE_AWS_REGION=us-east-1
VITE_AWS_ACCESS_KEY_ID=
VITE_AWS_SECRET_ACCESS_KEY=

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_OFFLINE_MODE=true

# UI Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_MAX_PROJECTS=50
VITE_AUTO_SAVE_INTERVAL=30000
VITE_DEFAULT_LANGUAGE=zh-CN

# External Services
VITE_IMAGE_SEARCH_API=https://api.unsplash.com/search/photos
VITE_IMAGE_API_KEY=your-unsplash-api-key

# Development Settings
VITE_MOCK_API=false
VITE_API_DELAY=0
VITE_LOG_LEVEL=info